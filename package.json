{
  "name": "Redux-react-kit",
  "version": "1.0.0",
  "description": "My kit",
  "main": "webpack.config.js",
  "scripts": {
    "start": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:stage": "better-npm-run build:stage"
  },
  "betterScripts": {
    "build:dev": {
      "command": "node dev.server.js",
      "env": {
        "env": "dev",
        "port": 3000
      }
    },
    "build:stage": {
      "command": "rimraf ./dist && webpack --config webpack.config.js",
      "env": {
        "env": "stage"
      }
    },
    "build:prod": {
      "command": "rimraf ./dist && webpack --config webpack.config.js",
      "env": {
        "env": "prod"
      }
    }
  },
  "author": "Denis Kozak",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "fixed-data-table": "^0.6.3",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "node-sass": "^3.10.1",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^3.0.0-beta.2",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "better-npm-run": "^0.0.15",
    "http-server": "^0.9.0",
    "moment": "^2.15.1",
    "normalize.css": "^5.0.0",
    "prop-types": "^15.5.10",
    "package": "^1.0.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.2",
    "redbox-react": "^1.3.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.0.0",
    "webpack-hot-middleware": "^2.18.0",
    "whatwg-fetch": "^1.0.0"
  },
  "repository": {
    "url": "git@github.com:mhaagens/SkyDropGG/mobx-react-kit.git",
    "type": "git"
  }
}
